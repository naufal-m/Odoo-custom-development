<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <!-- Sequences for om.hospital.patient.order. create a sequence order id into the 'ir.sequence' model -->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Hospital Patient</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">OMHP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for om.hospital.hospital.appointment. create a sequence order id into the 'ir.sequence' model -->
        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Hospital Appointment</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">OT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for om.hospital.doctor.order. create a sequence order id into the 'ir.sequence' model -->
        <record id="seq_hospital_doctor" model="ir.sequence">
            <field name="name">Hospital Doctor</field>
            <field name="code">hospital.doctor</field>
            <field name="prefix">OMHD</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for om.hospital.department.order. create a sequence order id into the 'ir.sequence' model -->
        <record id="seq_hospital_department" model="ir.sequence">
            <field name="name">Hospital Department</field>
            <field name="code">hospital.department</field>
            <field name="prefix">DDT</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>